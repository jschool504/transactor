    "node-tesseract-ocr": "2.2.1",
    "pg": "8.7.3",
    "plaid": "12.1.0",
    "pm2": "5.2.0",
    "ts-node": "10.5.0",
    "typescript": "4.5.5",
    "xml2js": "0.4.23"
